<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
 <description>GLD 1.0 library</description>
 <display-name>GldTag</display-name>
 <tlib-version>1.0</tlib-version>
 <short-name>gld</short-name>
 <tag>
  <name>gld</name>
  <tag-class>com.gcj.common.GldTag</tag-class>
  <body-content>empty</body-content>
  <attribute>
   <name>type</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>value</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <!-- 自定义函数 -->
 <function>
  <name>getDicValues</name>
  <function-class>com.gcj.common.Functions</function-class>
  <function-signature>java.util.List getDicValues(java.lang.String)</function-signature>
 </function>
 <function>
  <name>getDicName</name>
  <function-class>com.gcj.common.Functions</function-class>
  <function-signature>java.lang.String getDicName(java.lang.String,java.lang.String)</function-signature>
 </function>
 <function>
  <name>getDicTypeName</name>
  <function-class>com.gcj.common.Functions</function-class>
  <function-signature>java.lang.String getDicTypeName(java.lang.String,java.lang.String)</function-signature>
 </function>
 <function>
  <name>getUserName</name>
  <function-class>com.gcj.common.Functions</function-class>
  <function-signature>java.lang.String getUserName(java.lang.String)</function-signature>
 </function>
 <function>
  <name>isAuthority</name>
  <function-class>com.gcj.common.Functions</function-class>
  <function-signature>java.lang.Boolean isAuthority(java.lang.String,java.lang.String)</function-signature>
 </function>
</taglib>
